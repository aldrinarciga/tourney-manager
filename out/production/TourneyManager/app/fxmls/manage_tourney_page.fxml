<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.ManageTourneyPageController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="55.0" minHeight="55.0" prefHeight="55.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <content>
            <HBox fx:id="boardContainer" fillHeight="false" maxHeight="55.0" minHeight="55.0" prefHeight="55.0" prefWidth="200.0" spacing="10.0" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER_LEFT" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="200.0" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btnSave" minHeight="20.0" minWidth="100.0" mnemonicParsing="false" onAction="#saveMatch" text="Save" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnSearch" minHeight="20.0" minWidth="100.0" mnemonicParsing="false" onAction="#searchMatch" text="Search" />
            <Button fx:id="btnPrint" minHeight="20.0" minWidth="100.0" mnemonicParsing="false" onAction="#printMultiple" text="Print Multiple" />
         </children>
         <padding>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </padding>
      </HBox>
      <WebView fx:id="webMain" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
